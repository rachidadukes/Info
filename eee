 private static async Task TestAccountInquiryAsync(string serviceUrl)
        {
            // Binding â€“ starting point, we can adjust once we see errors
            var binding = new System.ServiceModel.BasicHttpsBinding(
                System.ServiceModel.BasicHttpsSecurityMode.Transport);

            binding.MaxReceivedMessageSize = 1024 * 1024; // adjust if needed

            var endpoint = new System.ServiceModel.EndpointAddress(serviceUrl);

            var client = new FinancialInquiryWsClient(binding, endpoint);

            await CallStAccountInquiryAsync(client);
        }

        private static async Task CallStAccountInquiryAsync(FinancialInquiryWsClient client)
