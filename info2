using static RequestsSamplesApp.FieldConfiguration; // Use your correct namespace

var orderedProps = props
    .OrderBy(p => Array.IndexOf(PreferredPropertyOrder, p.Name))
    .ThenBy(p => p.Name)
    .ToList();

