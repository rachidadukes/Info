===========================

public static class FieldConfiguration
{
    public static readonly string[] PreferredPropertyOrder =
    {
        "RqUID", "EnvironmentName", "TranCode", "BranchId", "PCName",
        "TranCodeQual", "CashBoxId", "EmployeeId",
        "PostingDt", "CalendarDt", "TranTime", "TranSeqNum",
        "ETSTranStatus", "OverrideCode", "OverrideEmployeeId",
        "ForwardTranFlag", "CustPermId", "POAInd",
        "MultiTranFlag", "ConductorIdType", "ConductorIdNum",
        "AcctId", "AcctType", "DepositType",
        "TransAmt", "CashInAmt", "CashOutAmt", "ScannedChkAmt", "UnscannedChkAmt", "NumberOfChecks",
        "FltLvl1", "FltLvl2", "FltLvl3",
        "EventID", "Environment", "RequestType"
    };

    public static readonly HashSet<string> HiddenFields = new()
    {
        "Id", "EventID"
    };

    public static readonly HashSet<string> ReadOnlyFields = new()
    {
        "RqUID", "PostingDt", "CalendarDt"
    };
}
