samples

using System;
using System.Linq;
using System.Xml.Linq;

public static class XmlExtractor
{
    public static void Main()
    {
        string xml = @"Send to Host; len=1150 09/23/2025 16:15:41.0865 <CreditAddRq><MsgRqHdr xmlns='http://schemas.navyfederal.org/2010/04/NFBranchExService'><RqUID>69be352b</RqUID></MsgRqHdr><EnvironmentName>NFCU</EnvironmentName><TranCode>2001</TranCode><TranCodeQual>0000</TranCodeQual></CreditAddRq>";

        // --- Clean invalid prefix ---
        int index = xml.IndexOf("<CreditAddRq>");
        if (index > 0)
            xml = xml.Substring(index);  // keep only the XML part

        // --- Parse XML safely ---
        XDocument doc = XDocument.Parse(xml);

        // --- Extract values (case-sensitive tag names) ---
        string tranCode = doc.Descendants("TranCode").FirstOrDefault()?.Value ?? "";
        string tranCodeQual = doc.Descendants("TranCodeQual").FirstOrDefault()?.Value ?? "";

        Console.WriteLine($"TranCode: {tranCode}");
        Console.WriteLine($"TranCodeQual: {tranCodeQual}");
    }
}
