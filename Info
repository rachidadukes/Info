public static string GetFirstTag(string xml)
{
    int startIndex = xml.IndexOf('<');
    int endIndex = xml.IndexOf('>');

    if (startIndex == -1 || endIndex == -1 || endIndex <= startIndex)
        return null;

    // Extract what's inside the angle brackets
    return xml.Substring(startIndex + 1, endIndex - startIndex - 1);
}
