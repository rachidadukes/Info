private void ExtractAccountsAndTypesAndAccessNums()
{
    // Convert ObservableCollection to List once
    var logsList = LogManager.EncoreLogs.ToList();

    // Account numbers
    var accts = new ExtractAccountNums();
    accountList = accts.ExtractAccountAndType(logsList);
    List<string> accounts = accountList.Select(x => x.AccountNumber).Distinct().ToList();

    // Request types
    var types = new ExtractTypes();
    requestTypesList = types.ReadRequestTypesOnly(logsList);
    List<string> typeList = requestTypesList.Select(x => x.Type).Distinct().ToList();

    // Access numbers
    var accessNums = new ExtractAccessNums();
    accessNumsList = accessNums.ExtractAccessNumbersOnly(logsList);
    List<string> accessList = accessNumsList.Select(x => x.AccessNumber).Distinct().ToList();

    // Bind to UI
    AccountListView.ItemsSource = accounts;
    RequestTypeListView.ItemsSource = typeList;
    AccessNumListView.ItemsSource = accessList;
}
