=====================================================

CREATE TABLE CreditAddRequest (
    Id INTEGER PRIMARY KEY AUTOINCREMENT,
    RqUID TEXT,
    EnvironmentName TEXT,
    TranCode TEXT,
    BranchId TEXT,
    PCName TEXT,
    TranCodeQual TEXT,
    CashBoxId INTEGER,
    EmployeeId INTEGER,
    PostingDt TEXT,
    CalendarDt TEXT,
    TranTime TEXT,
    TranSeqNum INTEGER,
    ETSTranStatus TEXT,
    OverrideCode INTEGER,
    OverrideEmployeeId INTEGER,
    ForwardTranFlag INTEGER,
    CustPermId INTEGER,
    POAInd INTEGER,
    MultiTranFlag INTEGER,
    ConductorIdType TEXT,
    ConductorIdNum TEXT
);
