DELETE FROM AccessAcctNumbers
WHERE Id NOT IN (
    SELECT MIN(Id)
    FROM AccessAcctNumbers
    GROUP BY AccessNumber, AccountNumber, AccountType, Environment
);

===================================

SELECT AccessNumber, AccountNumber, AccountType, Environment, COUNT(*) AS DuplicateCount
FROM AccessAcctNumbers
GROUP BY AccessNumber, AccountNumber, AccountType, Environment
HAVING COUNT(*) > 1;

================================

