private static string FixTranTime(string xml, string timeValue)
{
    // Replace ISO8601 DateTime with simple HH:mm:ss
    // regex: <TranTime>...</TranTime>
    return System.Text.RegularExpressions.Regex.Replace(
        xml,
        @"<TranTime>.*?</TranTime>",
        $"<TranTime>{timeValue}</TranTime>");
}


==================

// ===== FIX TRAN TIME HERE =====
xml = FixTranTime(xml, "14:23:21");
