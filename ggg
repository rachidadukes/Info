using ApiClientLib;

private static async Task<int> CallApi()
{
    var client = new ApiXmlClient(ignoreCertErrors: true);

    string endpointUrl = "https://your-api-endpoint.com/MessageConverter";
    string xml = RequestBuilder.BuildXmlRequest();

    string response = await client.PostXmlAsync(endpointUrl, xml);

    Console.WriteLine(response);
    return 0;
}
