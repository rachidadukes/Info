using SoapLegacyClient.HfsEncHostService;
using System;

namespace SoapLegacyClient
{
    internal class Program
    {
        static void Main(string[] args)
        {
            // Create proxy client
            var client = new HFS_EncHost();

            // Optional: override endpoint from App.config (useful for debugging)
            // client.Url = "http://localhost:2529/HFS_EncHost.asmx";

            // Replace these with test values
            string dbid = "test-db";
            string recordId = "some-id";

            try
            {
                // Call the service method
                string response = client.GetStateRateData(dbid, recordId);

                // Display the result
                Console.WriteLine("Host service response:");
                Console.WriteLine(response);
            }
            catch (Exception ex)
            {
                Console.WriteLine("Error calling SOAP service:");
                Console.WriteLine(ex.Message);
            }

            Console.ReadLine(); // Wait for user before closing
        }
    }
}
