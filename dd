var endpoint = sAPI_url.MemberSearch;   // replace with your actual URL constant
var soapXml = SoapStore.LoadRequestXml(); // your existing helper

var xmlClient = new MemberSearchLibrary.sMemberSearchXMLResponse();

try
{
    var response = await xmlClient.ReadXMLresponse(soapXml, endpoint);
    Console.WriteLine("Instance path succeeded, response length: " + response.Length);
    Console.WriteLine(response.Substring(0, Math.Min(400, response.Length)));
}
catch (Exception ex)
{
    Console.Error.WriteLine("Instance path failed: " + ex.ToString());
}
