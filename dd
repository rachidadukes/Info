using HostLogsWPFApp.Services;

public partial class MainWindow : Window
{
    private readonly MemberProfileService _memberProfileService;

    public MainWindow()
    {
        InitializeComponent();
        _memberProfileService = new MemberProfileService();
    }

    private async void BtnRetrieve_Click(object sender, RoutedEventArgs e)
    {
        var logAccount = new LogAccountsModel
        {
            AccessNumber  = "123456",
            AccountNumber = "987654",
            Environment   = "UNIT"
        };

        await _memberProfileService.RetrieveMembersProfile(logAccount, lblAccountsInfo);
    }
}
